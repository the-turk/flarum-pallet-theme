  "keywords": [
    "flarum",
    "theme",
    "pallet"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "authors": [{
    "name": "Hasan Ã–zbey",
    "email": "hasanoozbey@gmail.com",
    "homepage": "https://github.com/the-turk"
  }],
  "support": {
    "issues": "https://github.com/the-turk/flarum-pallet-theme/issues",
    "source": "https://github.com/the-turk/flarum-pallet-theme"
  },
  "require": {
    "flarum/core": "^1.0.2",
    "flarum/tags": "^1.0.3"
